            <string>/// CONSTRUIR MÁS TERRENO.

// if (wait &gt; 0) {wait--; exit}
// if (Block_Count &gt;= Block_Limit) {exit}

var ii,jj,dir;

// Elegir dirección.
switch (STYLE)
    {
    case WORLD_FLAT:        dir = choose(R,R,R,L,L,L,U); break;
    case WORLD_1_ISLAND:    dir = choose(R,L,U,D); break;
    case WORLD_2_ISLAND:    dir = choose(R,L,U,D); break;
    case WORLD_2_PILLAR:    dir = choose(R,L,U,U,U,U,D); break;
    case WORLD_4_PILLAR:    dir = choose(R,L,U,U,U,U,D); break;
    default:                dir = 0; break;
    }

// Posicionar.
switch (dir)
    {
    case R: ii = i+1; jj = j; break;
    case L: ii = i-1; jj = j; break;
    case U: ii = i; jj = j-1; break;
    case D: ii = i; jj = j+1; break;
    default: ii = i; jj = j; break;
    }

// Crear bloque.
terrain_build(ii,jj,i_min,i_max);
</string>
