            <kind>1</kind>
            <string>/// TRANSFORMARSE EN OTRO BLOQUE.

var i,block;

i = rir(0,99);

// Tipo de bloque.
block = obj_ground;
if (i &lt; BLOCKS[B_ATOMIC])
    {block = obj_atomic; i += 700}
i -= BLOCKS[B_ATOMIC];
if (i &lt; BLOCKS[B_HEALTH])
    {block = obj_health; i += 700}
i -= BLOCKS[B_HEALTH];
if (i &lt; BLOCKS[B_CHEST])
    {block = obj_chest; i += 700}
i -= BLOCKS[B_CHEST];
if (i &lt; BLOCKS[B_STONE])
    {block = obj_stone; i += 700}
i -= BLOCKS[B_STONE];

// Si debajo hay Aire o Agua se transforma en Ground.
if (!position_meeting(x,y+S,obj_block))
    {block = obj_ground}

// Metamorfosis forzada.
instance_create(x,y,block);
instance_destroy();
</string>
